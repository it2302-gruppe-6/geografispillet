%h1 Lister opp brukere

%table
  %tr
    %th Brukernavn
    %th Epost

  - @users.each do |user|
    %tr
      %td= user.username
      %td= user.email
      %td= link_to 'Vis', user
      %td= link_to 'Endre', edit_user_path(user)
      %td= link_to 'Slett', user, :confirm => 'Er du sikker?', :method => :delete

%br
= link_to 'Autentiseringer', authentications_path
= link_to 'Ny bruker', new_user_path
